@model List<TextEditor>
@{
    ViewData["Title"] = "List";
}

<div class="container mt-5">
    <h1 class="text-center">List of Files</h1>
     @foreach (var obj in Model)
        {
        <div class="col-md-4 mb-3">
            <button type="button" class="btn btn-primary btn-block">@obj.Name</button>
        </div>
        <hr/>
        }
</div>
